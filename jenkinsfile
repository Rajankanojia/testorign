pipeline {
    agent any
    stages {
        stage("init") {
            steps {
               echo 'init the application...'
            }
        }
        stage("build") {
            steps {
               eccho 'Building the application...'
            }
        }
        stage("test") {
           steps {
               echo 'test the application...'
            }
         }
        stage("deploy") {
            steps {
               echo 'deploy the application...'
            }
        }
    }   
}
